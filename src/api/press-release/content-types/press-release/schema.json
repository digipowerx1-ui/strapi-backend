{
  "kind": "collectionType",
  "collectionName": "press_releases",
  "info": {
    "singularName": "press-release",
    "pluralName": "press-releases",
    "displayName": "press-release"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "content": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "General,",
        " Events,",
        " Partnerships,",
        " Financials"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "pdfFile": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
